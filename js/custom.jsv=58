$(document).ready(function(){$('.slidelink').click(function(){var slug=$(this).attr('sl');window.location.href="/sto/"+slug;});$('.modulesSubNavLinks').find('a').click(function(){$('.modulesSubNavLinks').find('a').removeClass('on-link');$(this).addClass('on-link');gotoSection=$(this).attr('href');});$('.sub-links').find('a').click(function(){$('.sub-links').find('a').removeClass('on-link');$(this).addClass('on-link');gotoSection=$(this).attr('href');$('#aup').show();$('#menu-mobile').removeClass('open');});$('#acceptcookie').click(function(){$.cookie('gdrpCookie','accepted',{expires:365,path:'/'});$('#compliance').modal('hide');});if($(window).width()<900){$('.navbar').css('margin-top',0);}});let bAd;setBanner();$(window).scroll(function(){banner_scroll_interval();});$(window).scroll(function(){if(banner_scroll_interval){clearInterval(banner_scroll_interval);}});function banner_scroll_interval(){setInterval(function(){bAd=$(this).scrollTop();setBanner();},20);}
function setBanner(){console.log('eh');if(bAd==undefined){bAd=$(this).scrollTop();}
if(bAd>20){$('.navbar').css('margin-top',0);$('.navbar').addClass('sticky');}
if(bAd<=20){if($(window).width()>900){$('.navbar').css('margin-top',"40px");}
$('.navbar').removeClass('sticky');}}